{
  "editor.rulers": [100],
  "files.watcherExclude": {
    "node_modules/*": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/bower_components/**": true,
    "**/vendor/**": true,
    "**/platforms/**": true,
    "**/plugins/**": true,
    "**/dist/**": true,
    "**/.cache/**": true
  },
  "files.exclude": {
    "**/node_modules/**": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
  },
  "editor.renderWhitespace": "boundary"
}
